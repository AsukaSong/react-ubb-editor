{
  "name": "react-ubb-editor",
  "version": "1.0.0",
  "description": "A ubb editor component based on react",
  "browser": "./lib/index.js",
  "scripts": {
    "precommit": "lint-staged",
    "start": "start-storybook -p 9000 -c .storybook",
    "build": "rimraf lib && tsc",
    "test": "jest"
  },
  "keywords": [
    "react",
    "ubb",
    "editor"
  ],
  "homepage": "https://github.com/AsukaSong/react-ubb-editor",
  "repository": {
    "type": "git",
    "url": "https://github.com/AsukaSong/react-ubb-editor"
  },
  "files": [
    "lib"
  ],
  "author": "AsukaSong",
  "license": "WTFPL",
  "devDependencies": {
    "@storybook/addon-actions": "4.0.0-alpha.21",
    "@storybook/addon-info": "4.0.0-alpha.21",
    "@storybook/cli": "4.0.0-alpha.21",
    "@storybook/react": "4.0.0-alpha.21",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "@types/react": "^16.4.14",
    "@types/react-color": "^2.13.6",
    "@types/react-dom": "^16.0.7",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-beta.41",
    "babel-loader": "^7.0.0",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "lodash": "^4.17.11",
    "lodash-decorators": "^6.0.0",
    "rc-notification": "^3.2.0",
    "react": "^16.5.1",
    "react-color": "^2.14.1",
    "react-dom": "^16.5.1",
    "tslib": "^1.9.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
}
